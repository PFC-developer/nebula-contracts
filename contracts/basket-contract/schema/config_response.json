{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ConfigResponse",
  "type": "object",
  "required": [
    "config"
  ],
  "properties": {
    "config": {
      "$ref": "#/definitions/BasketConfig"
    }
  },
  "definitions": {
    "BasketConfig": {
      "type": "object",
      "required": [
        "assets",
        "basket_token",
        "name",
        "oracle",
        "owner",
        "penalty_params"
      ],
      "properties": {
        "assets": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/HumanAddr"
          }
        },
        "basket_token": {
          "$ref": "#/definitions/HumanAddr"
        },
        "name": {
          "type": "string"
        },
        "oracle": {
          "$ref": "#/definitions/HumanAddr"
        },
        "owner": {
          "$ref": "#/definitions/HumanAddr"
        },
        "penalty_params": {
          "$ref": "#/definitions/PenaltyParams"
        }
      }
    },
    "FPDecimal": {
      "type": "string"
    },
    "HumanAddr": {
      "type": "string"
    },
    "PenaltyParams": {
      "type": "object",
      "required": [
        "a_neg",
        "a_pos",
        "s_neg",
        "s_pos"
      ],
      "properties": {
        "a_neg": {
          "$ref": "#/definitions/FPDecimal"
        },
        "a_pos": {
          "$ref": "#/definitions/FPDecimal"
        },
        "s_neg": {
          "$ref": "#/definitions/FPDecimal"
        },
        "s_pos": {
          "$ref": "#/definitions/FPDecimal"
        }
      }
    }
  }
}
